{
  "id": 11916613,
  "name": "Release Please Branches: Trusted Bots Only",
  "description": "Restricts access to release-please branches, allowing only trusted bots to create, update, or delete them, while enforcing code quality and signature requirements.",
  "target": "branch",
  "source_type": "Repository",
  "source": "TimSchoenle/actions",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [],
      "include": ["refs/heads/release-please--**"]
    }
  },
  "rules": [
    {
      "type": "deletion"
    },
    {
      "type": "creation"
    },
    {
      "type": "update"
    },
    {
      "type": "non_fast_forward"
    },
    {
      "type": "code_quality",
      "parameters": {
        "severity": "errors"
      }
    },
    {
      "type": "code_scanning",
      "parameters": {
        "code_scanning_tools": [
          {
            "tool": "CodeQL",
            "security_alerts_threshold": "high_or_higher",
            "alerts_threshold": "errors"
          }
        ]
      }
    },
    {
      "type": "required_signatures"
    },
    {
      "type": "copilot_code_review_analysis_tools",
      "parameters": {
        "tools": [
          {
            "name": "CodeQL"
          }
        ]
      }
    }
  ],
  "bypass_actors": [
    {
      "actor_id": 2427303,
      "actor_type": "Integration",
      "bypass_mode": "always"
    },
    {
      "actor_id": 2625109,
      "actor_type": "Integration",
      "bypass_mode": "always"
    }
  ]
}
