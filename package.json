{
  "name": "actions",
  "version": "1.0.0",
  "scripts": {
    "build:workspaces": "bun run --workspaces build",
    "create-action": "bun scripts/create-action.ts",
    "remove-action": "bun scripts/remove-action.ts",
    "create-workflow": "bun scripts/create-workflow.ts",
    "remove-workflow": "bun scripts/remove-workflow.ts",
    "generate-docs": "bun scripts/generate-docs.ts",
    "lint": "eslint scripts/**/*.ts",
    "format": "prettier --write .",
    "test": "vitest run --coverage",
    "prepare": "husky"
  },
  "type": "module",
  "devDependencies": {
    "@eslint/compat": "2.0.2",
    "@eslint/js": "10.0.1",
    "@fast-check/vitest": "0.2.4",
    "@inquirer/prompts": "8.2.1",
    "@types/bun": "1.3.9",
    "@types/js-yaml": "4.0.9",
    "@types/node": "25.3.0",
    "@typescript-eslint/eslint-plugin": "8.56.0",
    "@typescript-eslint/parser": "8.56.0",
    "@vitest/coverage-v8": "4.0.18",
    "ansi-escapes": "7.3.0",
    "chardet": "2.1.1",
    "cli-width": "4.1.0",
    "eslint": "10.0.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-no-secrets": "2.2.2",
    "eslint-plugin-perfectionist": "5.6.0",
    "eslint-plugin-security": "4.0.0",
    "eslint-plugin-sonarjs": "4.0.0",
    "eslint-plugin-unicorn": "63.0.0",
    "husky": "9.1.7",
    "iconv-lite": "0.7.2",
    "js-yaml": "4.1.1",
    "lint-staged": "16.2.7",
    "mute-stream": "3.0.0",
    "prettier": "3.8.1",
    "run-async": "4.0.6",
    "rxjs": "7.8.2",
    "signal-exit": "4.1.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0",
    "vitest": "4.0.18",
    "wrap-ansi": "10.0.0"
  },
  "dependencies": {
    "chalk": "5",
    "inquirer": "^13.0.0"
  },
  "workspaces": [
    "actions/**/*",
    "workflows/**/*"
  ]
